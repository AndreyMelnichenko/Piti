<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Mobox">
<listeners>
<listener class-name="utils.RetryListener"/>
<listener class-name="utils.TestListener"/>
</listeners>
    <test name="API Tests" preserve-order="false" verbose="1">
        <classes>
            <class name="BrowsersTests.BasicUserBehaveTest"/>
        </classes>
    </test>
    <packages>
        <package name="ApiTests.PitiApiTest">
            <exclude name="*"/>
        </package>
    </packages>
</suite>
